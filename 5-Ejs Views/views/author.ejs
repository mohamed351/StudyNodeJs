<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Author</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
        </thead>
        <body>
            <% data.forEach(function(a){ %>
                <tr>
                    <td> <%=a.id %> </td>
                    <td> <%=a.name %> </td>
                    
                </tr>
              <% }) %>  
        </body>
    </table>
    
</body>
</html>